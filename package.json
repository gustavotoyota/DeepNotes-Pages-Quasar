{
  "name": "deepnotes-pages",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "DeepNotes",
  "author": "Gustavo Takachi Toyota <gustavotakachitoyota@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m ssr",
    "build": "quasar build",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "postinstall": "yarn patch-package",
    "test:unit:ui": "majestic",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "@syncedstore/core": "^0.3.7",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.6",
    "highlight.js": "^11.5.1",
    "idb": "^7.0.1",
    "js-base64": "^3.7.2",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "patch-package": "^6.4.7",
    "pinia": "^2.0.11",
    "postinstall-postinstall": "^2.1.0",
    "quasar": "^2.6.0",
    "quill": "^1.3.7",
    "quill-cursors": "^3.1.2",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.1.0",
    "y-indexeddb": "^9.0.7",
    "y-quill": "^0.1.4",
    "y-websocket": "^1.4.0",
    "yjs": "^13.5.34",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0-alpha.0",
    "@quasar/quasar-app-extension-testing": "^2.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^12.20.21",
    "@types/quill": "^2.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "majestic": "^1.7.0",
    "prettier": "^2.5.1",
    "quill-delta": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
